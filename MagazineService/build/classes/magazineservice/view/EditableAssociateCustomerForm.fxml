<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>

<BorderPane xmlns:fx="http://javafx.com/fxml/1" fx:id="editableForm" 
      fx:controller="magazineservice.controller.EditableAssociateCustomerFormController">
    <center>
        <VBox fx:id="fieldContainer" spacing="15.0f">
            <children>
                <HBox fx:id="nameField" spacing="5.0f">
                    <Label fx:id="nameFieldLabel" text="Name: " alignment="CENTER_LEFT"></Label>
                    <TextField fx:id="nameFieldTextBox"></TextField>
                </HBox>
                <HBox fx:id="emailField" spacing="5.0f">
                    <Label fx:id="emailFieldLabel" text="Email: " alignment="CENTER_LEFT"></Label>
                    <TextField fx:id="emailFieldTextBox"></TextField>
                </HBox>
                <HBox fx:id="typeField" spacing="5.0f">
                    <Label fx:id="typeFieldLabel" text="Type: " alignment="CENTER_LEFT"></Label>
                    <Label fx:id="associateTypeLabel" text="Associate Customer"></Label>
                </HBox>
                <HBox fx:id="benefactorField" spacing="5.0f">
                    <Label fx:id="benefactorFieldLabel" text="Benefactor: " alignment="CENTER_LEFT"></Label>
                    <ComboBox fx:id="benefactorOptions"></ComboBox>
                </HBox>
                <fx:include source="SupplementMagazineSelector.fxml"/>
            </children>
            <BorderPane.margin>
                <Insets bottom="10.0f" left="10.0f" right="10.0f" top="10.0f" />
            </BorderPane.margin>
        </VBox>
    </center>
</BorderPane>
